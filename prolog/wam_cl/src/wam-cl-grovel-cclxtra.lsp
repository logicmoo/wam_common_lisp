(defx8632lapfunction %inb ((port arg_z))
  (mark-as-imm temp1)
  (unbox-fixnum port edx)
  (:byte #xec)                          ;inb (%dx),%al
  (mark-as-node temp1)
  (movzbl (% al) (% eax))
  (box-fixnum eax arg_z)
  (single-value-return))

  
(defx8632lapfunction %inw ((port arg_z))
  (mark-as-imm temp1)
  (unbox-fixnum port edx)
  (:byte #x66) (:byte #xed)             ;inw (%dx),%ax
  (mark-as-node temp1)
  (movzwl (% ax) (% eax))
  (box-fixnum eax arg_z)
  (single-value-return))

(defx8632lapfunction %inl ((port arg_z))
  (mark-as-imm temp1)
  (unbox-fixnum port edx)
  (:byte #xed)                          ;inl (%dx),%eax
